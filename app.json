{
  "expo": {
    "name": "multimodal",
    "slug": "multimodal",
    "version": "1.0.0",
    "orientation": "portrait",
    "entryPoint": "./index.js",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
